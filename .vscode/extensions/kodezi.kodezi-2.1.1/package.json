	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "kodezi.webview",
				"title": "Start new webview session"
			},
			{
				"command": "kodezi.chatbot",
				"title": "Start new chatbot session"
			},
			{
				"command": "kodezi.debugging",
				"title": "Debugging"
			},
			{
				"command": "kodezi.optimizing",
				"title": "Optimizing"
			},
			{
				"command": "kodezi.translating",
				"title": "Translating"
			},
			{
				"command": "kodezi.documenting",
				"title": "Generate Documentation"
			},
			{
				"command": "kodezi.coding",
				"title": "Generate Code"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "kodezi.debugging",
					"group": "7_modification",
					"when": "authenticatedvar"
				},
				{
					"command": "kodezi.optimizing",
					"group": "7_modification",
					"when": "authenticatedvar"
				},
				{
					"command": "kodezi.translating",
					"group": "7_modification",
					"when": "authenticatedvar"
				},
				{
					"command": "kodezi.documenting",
					"group": "7_modification",
					"when": "authenticatedvar"
				},
				{
					"command": "kodezi.coding",
					"group": "7_modification",
					"when": "authenticatedvar"
				}
			]
		},
		"icons": {
			"kodezi-logo-icon": {
				"description": "Kodezi icon",
				"default": {
					"fontPath": "./KodeziIcon.woff",
					"fontCharacter": "\\41"
				}
			}
		},
		"viewsContainers": {
			"activitybar": [
				{
					"id": "package-explorer",
					"title": "KODEZI CODE",
					"icon": "resources/package-explorer.svg"
				}
			]
		},
		"views": {
			"package-explorer": [
				{
					"id": "kodezi-ai-primary",
					"type": "webview",
					"name": "KODEZI AI"
				},
				{
					"id": "kodezi-ai-quick",
					"type": "webview",
					"name": "QUICK ACCESS"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"deploy": "vsce publish",
		"pre-release": "vsce publish --pre-release",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
